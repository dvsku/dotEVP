<Window x:Class="dotEVP.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:dotEVP.GUI.Views"
        mc:Ignorable="d"
        Title="dotEVP" Height="281" Width="800" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <TabControl Margin="5">
            <TabItem Header="Pack">
                <views:PackView x:Name="PackView" Margin="10"/>
            </TabItem>
            <TabItem Header="Unpack">
                <views:UnpackView x:Name="UnpackView" Margin="10"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>